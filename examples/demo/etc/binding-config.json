{
  "name": "afb-rust",
  "no-ldpaths": true,
  "port": 1234,
  "alias": ["/devtools:/usr/share/afb-ui-devtools/binder"],
  "monitoring": true,
  "cntxtimeout": 30,

  "binding": [
    {
        "uid": "rust-api",
        "path": "${CARGO_TARGET_DIR}debug/examples/libafb_demo.so",
        "info": "RUST sample API binding (Rust)"
    }
  ],

  "set" : {
    "rust-api": {
      "key": "any json for binding 'config' control callback"
    }
  }
}